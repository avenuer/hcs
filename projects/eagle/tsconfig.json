{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@ulmax/server-shared": [
        "libs/server-shared/src"
      ],
      "@ulmax/server-shared/*": [
        "libs/server-shared/src/*"
      ],
      "@ulmax/authentication": [
        "libs/authentication/src"
      ],
      "@ulmax/authentication/*": [
        "libs/authentication/src/*"
      ],
      "@ulmax/users-admininistration": [
        "libs/users-admininistration/src"
      ],
      "@ulmax/users-admininistration/*": [
        "libs/users-admininistration/src/*"
      ],
      "@ulmax/data-access-records": [
        "libs/data-access-records/src"
      ],
      "@ulmax/data-access-records/*": [
        "libs/data-access-records/src/*"
      ],
      "@ulmax/messaging": [
        "libs/messaging/src"
      ],
      "@ulmax/messaging/*": [
        "libs/messaging/src/*"
      ],
      "@ulmax/sip": [
        "libs/sip/src"
      ],
      "@ulmax/sip/*": [
        "libs/sip/src/*"
      ],
      "@ulmax/ehr": [
        "libs/ehr/src"
      ],
      "@ulmax/ehr/*": [
        "libs/ehr/src/*"
      ],
      "@ulmax/general-public": [
        "libs/general-public/src"
      ],
      "@ulmax/general-public/*": [
        "libs/general-public/src/*"
      ],
      "@ulmax/ehr-intercom": [
        "libs/ehr-intercom/src"
      ],
      "@ulmax/ehr-intercom/*": [
        "libs/ehr-intercom/src/*"
      ],
      "@ulmax/testing": [
        "libs/testing/src"
      ],
      "@ulmax/testing/*": [
        "libs/testing/src/*"
      ],
      "@ulmax/frontend": [
        "libs/frontend/src"
      ],
      "@ulmax/frontend/*": [
        "libs/frontend/src/*"
      ],
      "@ulmax/lota": [
        "libs/lota/src"
      ],
      "@ulmax/lota/*": [
        "libs/lota/src/*"
      ],
      "@ulmax/cardnode": [
        "libs/cardnode/src"
      ],
      "@ulmax/cardnode/*": [
        "libs/cardnode/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}