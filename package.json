    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "protobufjs": "^6.8.9",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.7",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
