{
  "name": "eagle",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon --watch libs/**/*.ts --exec npm start",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "webpack": "webpack --config ./webpack.config.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "frontend": "npm run frontend:build && npm run frontend:link",
    "frontend:build": "npx tsc -p ./libs/frontend/tsconfig.lib.json --OutDir ./libs/frontend/dist -d",
    "frontend:link": "cd ./libs/frontend && yarn link",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/eagle/test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/microservices": "^6.5.3",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/terminus": "^6.5.6",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "@sentry/node": "^5.10.2",
    "@types/lodash": "^4.14.149",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.5",
    "aws-sdk": "^2.597.0",
    "chance": "^1.1.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "factory.ts": "^0.5.1",
    "jest-ts-auto-mock": "^1.0.7",
    "mailgun-js": "^0.22.0",
    "mixpanel": "^0.11.0",
